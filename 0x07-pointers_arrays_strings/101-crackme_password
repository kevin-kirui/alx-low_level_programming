#include "main.h"
#include <stdio.h>

/**
 * main - check the code
 *
 * Return: Always 0.
 */
#include <stdio.h>

int main() 
{
FILE *file;
const char *password = "mypassword";
file = fopen("password.txt", "w");

if (file == NULL) 
{
printf("Unable to open the file.\n");
        return 1;
}
    for (const char *ch = password; *ch != '\0'; ch++) 
{
        putchar(*ch);
}
    fclose(file);

    return 0;
}
